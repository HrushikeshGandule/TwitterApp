<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>TwitterApp</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>TwitterServlet</servlet-name>
    <servlet-class>com.tweetsDemo.TwitterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TwitterServlet</servlet-name>
    <url-pattern>/twitterservlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Tweets</display-name>
    <servlet-name>Tweets</servlet-name>
    <servlet-class>com.tweetsDemo.Tweets</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Tweets</servlet-name>
    <url-pattern>/tweets</url-pattern>
  </servlet-mapping>
</web-app>